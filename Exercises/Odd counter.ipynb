    "def odd_counter(A, B, C):\n",
    "    count = 0\n",
    "    while True:\n",
    "        count += 1\n",
    "        A, B, C = (B+C)/2, (C+A)/2, (A+B)/2\n",
    "        print(A, B, C)\n",
    "        if A % 2 != 0 or B % 2 != 0 or C % 2 != 0:\n",
    "            break\n",
    "    return count\n",
    "    \n",
    "odd_counter(2,6,6) "
